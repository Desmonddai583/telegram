<div class="header">
  <div class='home-link'>
    {{#link-to 'dashboard'}}<img src="images/icon-home.png"> <span>Home</span>{{/link-to}}
  </div>

  <div class='home-logo'>
    <img src="images/telegram-logo-header.png">
  </div>

  <div class='profile-link'>
    {{#link-to 'user' session.user.id}}
      <img {{bind-attr src=session.user.photo}} height="30" width="30"> 
      <span>{{session.user.name}}</span>
    {{/link-to}}
  </div>
</div>